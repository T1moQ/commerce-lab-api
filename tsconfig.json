{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"lib": ["ES2022"],
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"types": ["node"],
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"@prisma/client": ["./generated/prisma"]
		}
	},
	"include": ["src", "prisma", "generated"]
}
